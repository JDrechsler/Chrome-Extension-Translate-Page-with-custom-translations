{
	"manifest_version": 2,
	"name": "Translate Page",
	"description": "Translates a page using predefined language pairs.",
	"version": "1.0.0",
	"icons": {
		"128": "128.png"
	},
	"browser_action": {
		"default_title": "Inject!",
		"default_icon": "19.png"
	},
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"permissions": [
		"activeTab"
	]
}
